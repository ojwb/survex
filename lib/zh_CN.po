#: n:503
#, c-format
msgid "Datum “%s” not supported"
msgstr ""

#: ../src/commands.c:2071
#: n:309
msgid "Expected number or “AUTO”"
msgstr "需要数字或“AUTO”"

#: ../src/datain.c:3773
#: n:304
msgid "No survey date specified - using 0 for magnetic declination"
msgstr "未指定测量日期-使用0作为磁偏角"

