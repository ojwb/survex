#: ../src/extend.c:334
#: ../src/extend.c:377
#: ../src/extend.c:420
#: ../src/readval.c:197
#: ../src/readval.c:458
#: ../src/readval.c:465
#: n:28
msgid "Expecting station name"
msgstr "Ожидается имя пикета"

#: ../src/commands.c:2704
#: n:31
#, c-format
msgid "Found “%s”, expecting “EQUATES”, “EXPORTS”, or “PLUMBS”"
msgstr "Вместо “%s” ожидаются “EQUATES”, “EXPORTS” или “PLUMBS”"

#: ../src/commands.c:2710
#: n:32
#, c-format
msgid "Found “%s”, expecting “ON” or “OFF”"
msgstr "Вместо “%s” ожидаются “ON” или “OFF”"

#. TRANSLATORS: EQUATE is a command name, so shouldn’t be
#. translated.
#.
#. Here "station" is a survey station, not a train station.
#: ../src/commands.c:1416
#: n:33
msgid "Only one station in EQUATE command"
msgstr "Указано только одно имя пикета в команде EQUATE"

